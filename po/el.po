#: ../panel-plugin/menu.c:304
msgid "Clipboard is empty"
msgstr "Το πρόχειρο είναι κενό"

#: ../panel-plugin/menu.c:399
msgid "_Disable"
msgstr "_Απενεργοποίηση"

